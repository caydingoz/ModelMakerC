<Window x:Class="WpfApp.GUI.CustomControls.CustomDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp.GUI.CustomControls"
        mc:Ignorable="d"
        Title="CustomDialog" Height="120" Width="400">
	<Grid>
		<TextBlock Name="labelName" Margin="10,5,0,0"/>
		<TextBox Name="dialogInput" Text="{Binding DataContext.input}" Width="300" Height="20" Margin="0,10,50,0"/>
		<Button Click="ExtractModel" Width="40" Height="30" HorizontalAlignment="Right" Margin="0,10,10,0"/>
	</Grid>
</Window>
